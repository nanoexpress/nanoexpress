{
  "name": "nanoexpress",
  "version": "0.5.5",
  "description": "Nano-framework for Node.js powered by uWebSockets.js",
  "main": "build/nanoexpress.js",
  "module": "src/nanoexpress.js",
  "typings": "nanoexpress.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "watch": "nodemon build/server.js",
    "watch-dev": "NODE_ENV=development rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint ./src",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat ./tests/coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/nanoexpress.git"
  },
  "keywords": [
    "nano",
    "node",
    "fast",
    "uws",
    "websocket",
    "http",
    "c++"
  ],
  "author": "dalisoft",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dalisoft/nanoexpress/issues"
  },
  "homepage": "https://github.com/dalisoft/nanoexpress#readme",
  "dependencies": {
    "@dalisoft/events": "0.0.9",
    "@sifrr/server": "0.0.5",
    "ajv": "^6.10.0",
    "cookie": "^0.4.0",
    "fast-json-stringify": "^1.15.3",
    "json-strify": "0.0.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#binaries"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "ava": "^1.4.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "rollup": "^1.12.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-run": "^1.1.0"
  }
}
